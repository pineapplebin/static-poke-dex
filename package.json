{
  "name": "static-poke-dex",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && npm run smui-theme",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "deploy": "npm run build && node ./gh-pages.js",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "next",
    "@types/lodash": "^4.14.182",
    "csstype": "^3.1.0",
    "gh-pages": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.53.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@smui/button": "^6.0.0-beta.16",
    "@smui/circular-progress": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/fab": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/tab": "^6.0.0-beta.16",
    "@smui/tab-bar": "^6.0.0-beta.16",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pokesprite-images": "^2.6.0",
    "smui-theme": "^6.0.0-beta.16"
  }
}
